<?xml version="1.0" encoding="utf-8"?>
<TextureAtlas>
	<Texture>images/atlas</Texture>
	<Regions>
		<Region name="slime-1" x="0" y="0" width="20" height="20"/>
		<Region name="slime-2" x="0" y="20" width="20" height="20"/>
		<Region name="bat-neutral" x="20" y="0" width="20" height="20"/>
		<Region name="bat-up" x="20" y="20" width="20" height="20"/>
		<Region name="bat-down" x="40" y="0" width="20" height="20"/>
	</Regions>
	<Animations>
		<Animation name="slime-animation" delay="200">
			<Frame region="slime-1" />
			<Frame region="slime-2" />
		</Animation>
		<Animation name="bat-animation" delay="200">
			<Frame region="bat-neutral" />
			<Frame region="bat-up" />
			<Frame region="bat-neutral" />
			<Frame region="bat-down" />
		</Animation>
	</Animations>
</TextureAtlas>
